/dts-v1/;
#include "sun8i-r40.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "Vutlan VT960v1";
	compatible = "vutlan,vt960v1", "allwinner,sun8i-r40";

	aliases {
		ethernet0 = &gmac;
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
	
	gpadc: gpadc@1c25000 {
		compatible = "allwinner,sun8i-r40-gpadc4ch";
		reg = <0x01c25000 0x400>;
		clocks = <&ccu CLK_BUS_TS>, <&ccu CLK_TS>, <&ccu CLK_DRAM_TS>;
		resets = <&ccu RST_BUS_TS>;
		interrupts = <GIC_SPI 29 IRQ_TYPE_LEVEL_HIGH>;
		status = "okay";
	};
	
	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		/* Address must be kept in the lower 256 MiBs of DRAM for VE. */
		default-pool {
			compatible = "shared-dma-pool";
			size = <0x6000000>;
			alloc-ranges = <0x4a000000 0x6000000>;
			reusable;
			linux,cma-default;
		};
		/* memory region for cedar video codec */
		ve_reserved: cma@0x50000000 {
			compatible = "shared-dma-pool";
			size = <0x5000000>;
			alloc-ranges = <0x50000000 0x5000000>;  /* 80M */
			reusable;
		};
	};
	
	/*
	ve: ve@01c0e000 {
		compatible = "allwinner,sunxi-cedar-ve";
		reg = <0x0 0x01c0e000 0x0 0x1000>,
		      <0x0 0x01c00000 0x0 0x10>,
		      <0x0 0x01c20000 0x0 0x800>;
		interrupts = <GIC_SPI 53 4>;
		clocks = <&clk_pll_ve>,
			 <&clk_ve>;

	};*/
	/*
	ve: video-engine@01c0e000 {
		compatible = "allwinner,sunxi-cedar";
		reg = <0x01c0e000 0x1000>;
		memory-region = <&ve_reserved>;

		clocks = <&ccu CLK_BUS_VE>, <&ccu CLK_VE>,
			<&ccu CLK_DRAM_VE>;
		clock-names = "ahb", "mod", "ram";
		assigned-clocks = <&ccu CLK_VE>;
		assigned-clock-rates = <320000000>;

		resets = <&ccu RST_BUS_VE>;
		interrupts = <GIC_SPI 53 IRQ_TYPE_LEVEL_HIGH>;			
	};*/
	
	tvd_top: tvd-top@01c30000 {
		compatible = "allwinner,sunxi-tvd";
		reg = <0x01c30000 0x800>;
		clocks = <&ccu CLK_BUS_TVD_TOP>, <&ccu CLK_DRAM_TVD>;
		resets = <&ccu RST_BUS_TVD_TOP>;
		interrupts = <GIC_SPI 61 0x0104>;
		tvd-number = <1>;
		tvds = <&tvd0>;
		status = "okay";			
	};	
	tvd0: tvd0@01c31000 {
		compatible = "allwinner,sunxi-tvd0";
		reg = <0x01c31000 0x1000>;
		interrupts = <GIC_SPI 97 0x0104>;
		clocks = <&ccu CLK_TVD0>, <&ccu CLK_BUS_TVD0>;
		resets = <&ccu RST_BUS_TVD0>;
		tvd_used = <1>;
		tvd_if = <0>;
		status = "okay";
	};
	tvd1: tvd1@01c32000 {
		compatible = "allwinner,sunxi-tvd1";
		reg = <0x01c32000 0x1000>;
		interrupts = <GIC_SPI 98 0x0104>;
		clocks = <&ccu CLK_TVD1>, <&ccu CLK_BUS_TVD1>;
		resets = <&ccu RST_BUS_TVD1>;
		tvd_used = <1>;
		tvd_if = <0>;
		status = "okay";
	};
	tvd2: tvd2@01c33000 {
		compatible = "allwinner,sunxi-tvd2";
		reg = <0x01c33000 0x1000>;
		interrupts = <GIC_SPI 99 0x0104>;
		clocks = <&ccu CLK_TVD2>, <&ccu CLK_BUS_TVD2>;
		resets = <&ccu RST_BUS_TVD2>;
		tvd_used = <1>;
		tvd_if = <0>;
		status = "okay";
	};
	tvd3: tvd3@01c34000 {
		compatible = "allwinner,sunxi-tvd3";
		reg = <0x01c34000 0x1000>;
		interrupts = <GIC_SPI 100 0x0104>;
		clocks = <&ccu CLK_TVD3>, <&ccu CLK_BUS_TVD3>;
		resets = <&ccu RST_BUS_TVD3>;
		tvd_used = <1>;
		tvd_if = <0>;
		status = "okay";
	};
	
	v4in1: v4in1@01c30000 {
		compatible = "allwinner,sunxi-v4in1";
		reg = <0x01c30000 0x01000>,/*tvd top*/
			  <0x01c31000 0x01000>,/*tvd0*/
			  <0x01c32000 0x01000>,/*tvd1*/
			  <0x01c33000 0x01000>,/*tvd2*/
			  <0x01c34000 0x01000>;/*tvd3*/
		clocks = <&ccu CLK_BUS_TVD_TOP>, <&ccu CLK_DRAM_TVD>,
				<&ccu CLK_TVD0>, <&ccu CLK_TVD1>, <&ccu CLK_TVD2>, <&ccu CLK_TVD3>;
		resets = <&ccu RST_BUS_TVD_TOP>,
				<&ccu RST_BUS_TVD0>, <&ccu RST_BUS_TVD1>, <&ccu RST_BUS_TVD2>, <&ccu RST_BUS_TVD3>;		
		interrupts = <GIC_SPI 97 0x0104>;
		status = "okay";
	};

	codec: codec@1c22c00 {
		#sound-dai-cells = <0>;
		compatible = "allwinner,sun8i-r40-codec";
		reg = <0x01c22c00 0x300>;
		interrupts = <GIC_SPI 30 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&ccu CLK_BUS_CODEC>, <&ccu CLK_AC_DIG>; 
		clock-names = "apb", "codec";
		resets = <&ccu RST_BUS_CODEC>;
		dmas = <&dma 19>, <&dma 19>;
		dma-names = "rx", "tx";
		status = "okay";

		allwinner,audio-routing =
			"Headphone", "HP",
			"Headphone", "HPCOM",
			"MIC1", "Mic",
			"MIC2", "Mic",
			"Mic",  "MBIAS",
			"LINEIN", "Line In";		
		allwinner,codec-analog-controls = <&codec_analog>;
	};	
	codec_analog: codec-analog@1c22f00 {
		compatible = "allwinner,sun8i-r40-codec-analog";
		reg = <0x01c22f00 0x4>;
	};

		/* bsp 4.4 */
//	codec:codec@0x01c22c00 {
//		compatible = "allwinner,sunxi-internal-codec";
//		reg = <0x01c22c00 0x2bc>,/*digital baseadress*/
//		      <0x01c22f00 0x4>;/*analog baseadress*/
//		clocks = <&clk_pll_audio>,<&clk_adda>;
//		headphonevol =  <0x3b>;
//		spkervol =  <0x1b>;
//		maingain =  <0x4>;
//		hp_dirused = <0x0>;
//		pa_sleep_time = <0x15e>;
//		status = "okay";
//	};

	lradc: lradc@1c24400 {
		compatible = "allwinner,sun4i-a10-lradc-keys";
		reg = <0x01c24400 0x100>;
		interrupts = <GIC_SPI 31 IRQ_TYPE_LEVEL_HIGH>;
		vref-supply = <&reg_aldo3>;
		status = "okay";
			
		button-248 {
			label = "Unmount";
			linux,code = <KEY_VOLUMEDOWN>;
			channel = <0>;
			voltage = <190476>;	/* 248211*2/3 */
		};	
		button-1169 {
			label = "Recover";
			linux,code = <KEY_ESC>;
			channel = <0>;
			voltage = <1111111>; /* 1169727*2/3 */
		};
	};

	usb_otg: usb@1c13000 {
		compatible = "allwinner,sun8i-r40-musb";
		reg = <0x01c13000 0x0400>;
		clocks = <&ccu CLK_BUS_OTG>;
		resets = <&ccu RST_BUS_OTG>;
		interrupts = <GIC_SPI 38 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "mc";
		phys = <&usbphy 0>;
		phy-names = "usb";
		extcon = <&usbphy 0>;
		allwinner,sram = <&otg_sram 1>;
		status = "disabled";
	};

	leds {
		compatible = "gpio-leds";
		
		hdd {
			label = "hdd-activity";
			gpios = <&pio 8 4 GPIO_ACTIVE_HIGH>; /* PI4 */
			/*linux,default-trigger = "disk-activity";*/
		};

		err {
			label = "error";
			gpios = <&pio 7 26 GPIO_ACTIVE_HIGH>; /* PH26 */
		};

		status {
			label = "status";
			gpios = <&pio 8 15 GPIO_ACTIVE_HIGH>; /* PI15 */
			default-state = "on";
			linux,default-trigger = "heartbeat";
		};

		can-bus {
			label = "can-bus";
			gpios = <&pio 1 9 GPIO_ACTIVE_HIGH>; /* PB9 */
		};

		relay1 {
			label = "relay1";
			gpios = <&pio 6 2 GPIO_ACTIVE_HIGH>; /* PG2 */
		};
		relay2 {
			label = "relay2";
			gpios = <&pio 6 3 GPIO_ACTIVE_HIGH>; /* PG3 */
		};
		relay3 {
			label = "relay3";
			gpios = <&pio 6 4 GPIO_ACTIVE_HIGH>; /* PG4 */
		};
		relay4 {
			label = "relay4";
			gpios = <&pio 6 5 GPIO_ACTIVE_HIGH>; /* PG5 */
		};

		usb0-pwr {
			label = "usb0-pwr";  /* usb 0 (OTG) */
			gpios = <&pio 8 13 GPIO_ACTIVE_HIGH>; /* PI13 */
			default-state = "on";
		};
		usb1-pwr {
			label = "usb1-pwr";  /* usb 1 */
			gpios = <&pio 1 12 GPIO_ACTIVE_HIGH>; /* PB12 */
			default-state = "on";
		};
		usb2-pwr {
			label = "usb2-pwr";  /* internal lte modem */
			gpios = <&pio 1 4 GPIO_ACTIVE_HIGH>; /* PB4 */
			default-state = "off";
		};
	};

	cpu0_opp_table: opp_table0 {
		compatible = "operating-points-v2";
		opp-shared;

		opp-528000000 {
			opp-hz = /bits/ 64 <528000000>;
			opp-microvolt = <1000000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};

		opp-720000000 {
			opp-hz = /bits/ 64 <720000000>;
			opp-microvolt = <1000000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};

		opp-912000000 {
			opp-hz = /bits/ 64 <912000000>;
			opp-microvolt = <1100000 1100000 1300000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};

		opp-1008000000 {
			opp-hz = /bits/ 64 <1008000000>;
			opp-microvolt = <1160000 1160000 1300000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};

		opp-1104000000 {
			opp-hz = /bits/ 64 <1104000000>;
			opp-microvolt = <1240000 1240000 1300000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};

		opp-1200000000 {
			opp-hz = /bits/ 64 <1200000000>;
			opp-microvolt = <1300000>;
			clock-latency-ns = <244144>; /* 8 32k periods */
		};
	};
};

&cpu0 {
	clocks = <&ccu CLK_CPU>;
	clock-names = "cpu";
	cpu-supply = <&reg_dcdc2>;
	operating-points-v2 = <&cpu0_opp_table>;
	clock-frequency = <912000000>;
};
&cpu1 {
	clocks = <&ccu CLK_CPU>;
	clock-names = "cpu";
	cpu-supply = <&reg_dcdc2>;
	operating-points-v2 = <&cpu0_opp_table>;
	clock-frequency = <912000000>;
};
&cpu2 {
	clocks = <&ccu CLK_CPU>;
	clock-names = "cpu";
	cpu-supply = <&reg_dcdc2>;
	operating-points-v2 = <&cpu0_opp_table>;
	clock-frequency = <912000000>;
};
&cpu3 {
	clocks = <&ccu CLK_CPU>;
	clock-names = "cpu";
	cpu-supply = <&reg_dcdc2>;
	operating-points-v2 = <&cpu0_opp_table>;
	clock-frequency = <912000000>;
};

&i2c0 {
	status = "okay";

	axp22x: pmic@34 {
		compatible = "x-powers,axp221";
		reg = <0x34>;
		interrupt-parent = <&nmi_intc>;
		interrupts = <0 IRQ_TYPE_LEVEL_LOW>;
	};
};

&i2c4 {
	clock-frequency = <100000>;
	status = "okay";
	
	mcp79400: mcp79400@6f {
		compatible = "microchip,mcp7940x";
		reg = <0x6f>;
	};
	
	/* TODO: vtx16, vtx40 */
};

/* console */ 
&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart0_pb_pins>;
	status = "okay";
};

/* rs485 */
&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart5_pins_vt960v1>;
	status = "okay";
};

/* 1-wire */
&uart7 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart7_pins_vt960v1>;
	status = "okay";
};

&spi0 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi0_cs0_pc_pin>, <&spi0_pc_pins>;
	status = "okay";
	dmas = <&dma 24>, <&dma 24>;
    dma-names = "rx", "tx";

	flash: m25p80@0 {
		compatible = "winbond,w25q64", "jedec,spi-nor";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>; /* Chip select 0 */	
		spi-max-frequency = <50000000>;

		partition@0 {
			label = "spl";
			reg = <0x0 0x10000>;	/* 64KB */
		};
		partition@10000 {
			label = "filesystem";
			reg = <0x10000 0>;
		};
	};
};
&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi2_pins_vt960v1>, <&spi2_cs_pin_vt960v1>;
	status = "okay";
	dmas = <&dma 26>, <&dma 26>;
    dma-names = "rx", "tx";

	spi@0 {
		compatible = "linux,spidev";
		reg = <0>;
		spi-max-frequency = <10000000>;
	};
};

&ahci {
	ahci-supply = <&reg_dldo4>;
	phy-supply = <&reg_eldo2>; 
	status = "okay";
};

&de {
	status = "okay";
};

&tcon_tv0 {
	status = "okay";
};

&can0 {
	pinctrl-names = "default";
	pinctrl-0 = <&can_ph_pins>;
	status = "okay";
};

#include "axp22x.dtsi"

&reg_dcdc1 {
	regulator-always-on;
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-3v3";
};

&reg_dcdc2 {
	regulator-always-on;
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1300000>;
	regulator-name = "vcc-cpu";
};

&reg_dcdc3 {
	regulator-always-on;
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1300000>;
	regulator-name = "vdd-sys";
};

&reg_dcdc4 {
	regulator-always-on;
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1300000>;
	regulator-name = "dcdc4";
};

&reg_dcdc5 {
	regulator-always-on;
	/* ddr3 Supply voltage operating range */
	regulator-min-microvolt = <1425000>;
	regulator-max-microvolt = <1575000>;
	regulator-name = "vcc-dram";
};

/* vcc-tvin vcc-25 */
&reg_aldo1 {
	regulator-always-on;
	regulator-min-microvolt = <2790000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-tvin";
};

/* vcc-pc vcc-emmcvq18 */
&reg_aldo2 {
	regulator-always-on;
	regulator-min-microvolt = <1620000>;
	regulator-max-microvolt = <1980000>;
	regulator-name = "vcc-emmcvq18";
};

&reg_aldo3 {
	regulator-always-on;
	regulator-min-microvolt = <3000000>;
	regulator-max-microvolt = <3100000>;
	regulator-name = "avcc";
};

/* vcc-io-wifi vcc-pg */
&reg_dldo1 {
	regulator-always-on;
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-wifi-io-vcc-pg";
};

&reg_dldo2 {
	regulator-always-on;
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-wifi";
};

&reg_dldo3 {
	regulator-always-on;
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "wlan-power-ext";
};

/* vcc-sata-25 vcc-pf */
&reg_dldo4 {
	regulator-always-on;
	regulator-min-microvolt = <2500000>;
	regulator-max-microvolt = <2500000>;
	regulator-name = "vcc-sata25-vcc-pf";
};

&reg_eldo1 {
	regulator-always-on;
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2800000>;
	regulator-name = "isp-dvdd";
};

/* vcc-sata-12 */
&reg_eldo2 {
	regulator-always-on;
	regulator-min-microvolt = <1200000>;
	regulator-max-microvolt = <1200000>;
	regulator-name = "vcc-sata12";
};

&reg_eldo3 {
	regulator-always-on;
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2800000>;
	regulator-name = "vcc-pe";
};

&reg_ldo_io0 {
	regulator-always-on;
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-ctp";
	status = "okay";
};

&reg_ldo_io1 {
	regulator-always-on;
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <3300000>;
	regulator-name = "vcc-i2s-18";
	status = "okay";
};

&reg_dc1sw {
	regulator-always-on;
	regulator-name = "vcc-lcd";
	status = "okay";
};

&ac_power_supply {
	status = "okay";
};



&pio {
	pinctrl-names = "default";
	pinctrl-0 = <&clk_out_a_pin>;

	 
	mmc0_cd_pin_vt960v1: mmc0_cd_pin_vt960v1@0 {
		pins = "PI11";
		function = "gpio_in";
		bias-pull-up;
	};
	
	uart5_pins_vt960v1: uart5_pins_vt960v1@0 {
		pins = "PH6", "PH7";
		function = "uart5";
	};
	uart7_pins_vt960v1: uart7_pins_vt960v1@0 {
		pins = "PI20", "PI21";
		function = "uart7";		
	};

	spi2_pins_vt960v1: spi2_pins_vt960v1@0 {
		pins = "PC20", "PC21", "PC22";
		function = "spi2";
	};
	spi2_cs_pin_vt960v1: spi2_cs_pin_vt960v1@0 {
		pins = "PC19";
		function = "spi2";
	};
	
	can_ph_pins: can-ph-pins {
		pins = "PH20", "PH21";
		function = "can";
	};

	usb0_id_detect_pin: usb0_id_detect_pin@0 {
		pins = "PI10";
		function = "gpio_in";
		bias-pull-up;
	};
	usb0_vbus_detect_pin: usb0_vbus_detect_pin@0 {
		pins = "PH17";
		function = "gpio_in";
	};

	gmac_pins_mii_a: gmac_pins_mii_a@0 {
		pins = "PA0", "PA1", "PA2",
		       "PA3", "PA4", "PA5", "PA6",
		       "PA7", "PA8", "PA9", "PA10",
		       "PA11", "PA12", "PA13", "PA14",
		       "PA15", "PA16", "PA17";
		function = "gmac";
		drive-strength = <30>;
	};
};

&gmac {
	pinctrl-names = "default";
	pinctrl-0 = <&gmac_pins_mii_a>;
	phy-handle = <&phy1>;
	phy-mode = "mii";
	phy-supply = <&reg_dcdc1>;
	status = "okay";
};
&gmac_mdio {
	phy1: ethernet-phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <1>;
		max-speed = <100>;
		reset-gpios = <&pio 7 22 GPIO_ACTIVE_LOW>; /* PH22 */
		reset-assert-us = <10000>;
		reset-deassert-us = <25000>;
	};
};

/* mmc0 card detect - ball V24 : gpio PI11 */
&mmc0 {
	pinctrl-names = "default";
	pinctrl-0 = <&mmc0_pins>, <&mmc0_cd_pin_vt960v1>;
	vmmc-supply = <&reg_dcdc1>;
	bus-width = <4>;
	cd-gpios = <&pio 8 11 GPIO_ACTIVE_HIGH>; 
	cd-inverted;
	no-1-8-v;
	status = "okay";
};

&mmc2 { 
	vmmc-supply = <&reg_dcdc1>;
	vqmmc-supply = <&reg_aldo2>;
	bus-width = <8>;
	non-removable;
	cap-mmc-hw-reset;	
	status = "okay";
	
	emmc: emmc@0 {
		reg = <0>;
		compatible = "mmc-card";
	};
};


&ehci0 {
	status = "okay";
};
&ohci0 {
	status = "okay";
};

&usb_otg {
	dr_mode = "host"; //"peripheral";
	status = "okay";
};

&ehci1 {
	status = "okay";
};
&ohci1 {
	status = "okay";
};

/* internal LTE modem */
&ehci2 {
	status = "okay";
};
&ohci2 {
	status = "okay";
};

&usb_power_supply {
	status = "okay";
};

&usbphy {
	pinctrl-names = "default";
	pinctrl-0 = <&usb0_id_detect_pin>, <&usb0_vbus_detect_pin>;

	usb0_id_det-gpio = <&pio 7 21 GPIO_ACTIVE_HIGH>; /* PH21 */
	usb0_vbus_det-gpios = <&pio 7 17 GPIO_ACTIVE_HIGH>; /* PH17 */
	//usb0_vbus-supply = <&reg_usb0_pwr>;
	usb0_vbus_power-supply = <&usb_power_supply>;

	//usb1_vbus-supply = <&reg_usb1_drv>;
	//usb2_vbus-supply = <&reg_usb2_drv>;

	status = "okay";
